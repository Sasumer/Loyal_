@model IEnumerable<Infraestructure.Models.ENC_FACTURA>

@{
    ViewBag.Title = "IndexAdmin";
}

<h2>Lista de Ordenes registradas</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.USUARIO.Nombre)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FECHA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.USUARIO.Nombre)
            </td>
            <td>
                @Html.ActionLink("Detalle", "Details", new { id = item.ID })
            </td>
        </tr>
    }

</table>

